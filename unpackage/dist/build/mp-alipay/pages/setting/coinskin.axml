<view class="container"><view class="panel"><view class="panel-title"><text>十二生肖系列</text></view><view class="cell"><block a:for="{{coins}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['selectCoin',['$0',index],[[['coins','',index]]]]]]]}}" class="{{(('coin-box')+' '+((coinIndex===index)?'selectActive':''))}}" onTap="__e"><image class="coin-img" src="{{item.src}}" mode="aspectFit"></image><text>{{item.name}}</text><block a:if="{{item.id===coinName}}"><text class="coin-spec">使用中</text></block></view></block></view><block a:if="{{coinIndex!==-1}}"><view class="footer"><button class="footer-btn" style="background:#fd746c;" type="primary" data-event-opts="{{[['tap',[['setCoin']]]]}}" onTap="__e">立即使用</button></view></block></view></view>