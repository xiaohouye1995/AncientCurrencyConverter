.container{width:100%;height:100vh;font-family:PingFang SC,Helvetica Neue,Arial,sans-serif;background-color:#22202e;color:#353535;font-size:14pt;position:relative;overflow:auto;padding:0 40rpx;box-sizing:border-box}.iconfont{font-size:11pt;color:#888}.icon-wenhao{font-size:14pt;color:#fff}.icon-money{width:100rpx;height:100rpx;line-height:100rpx;color:#fff;background-image:-webkit-linear-gradient(110deg,#fc6076,#ff9a44);background-image:linear-gradient(-20deg,#fc6076,#ff9a44);border-radius:50%;font-size:14pt;text-align:center}.btn-feedback{margin:0;padding:0;background:none;text-align:center;border:none;color:#fff}.btn-feedback:after{border:none}.note{font-size:11pt;color:#888;margin:0 8rpx}.panel-bg{width:100%;height:200px;text-align:center;font-size:20pt;color:#fff}.panel-bg-logo{width:200rpx;height:200rpx}.panel-bg-text{margin-top:20rpx;font-size:11pt}.panel{margin-bottom:20px;background-color:#fff;font-size:13pt;color:#353535;border-radius:12px;box-shadow:0 4px 9px 0 rgba(109,107,107,.5)}.panel-title{padding:20rpx;background-image:-webkit-linear-gradient(left,#f9d423,#ff4e50);background-image:linear-gradient(90deg,#f9d423 0,#ff4e50);border-top-left-radius:12px;border-top-right-radius:12px;color:#fff;font-size:14pt;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-pack:justify;-webkit-justify-content:space-between;justify-content:space-between}.panel-cell{padding:40rpx 20rpx;box-sizing:border-box;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-align:center;-webkit-align-items:center;align-items:center;border-bottom:1px dashed #ccc}.article{padding:40rpx 30rpx}.panel-cell:last-child{border:none}.panel-cell-bd{margin:0 12rpx}.panel-cell-ft{width:150rpx;-webkit-box-flex:1;-webkit-flex-grow:1;flex-grow:1;text-align:right}.minsize{font-size:11pt}