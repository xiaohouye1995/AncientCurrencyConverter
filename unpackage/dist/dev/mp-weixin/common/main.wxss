}
.container {
	font-family: PingFang SC, 'Helvetica Neue', Arial, sans-serif;
	background-color: #fff;
	color: #353535;
	font-size: 14pt;
	position: relative;
	padding-bottom: 40px;
}
.panel-bg {
	padding-bottom: 40px;
	background-color: #2c2933;
	text-align: center;
	font-size: 20pt;
	color: #fff;
}
.panel-bg-logo {
	width: 200rpx;
	height: 200rpx;
}
.panel-bg-text {
	margin-top: 20rpx;
	font-size: 13pt;
}
.panel-link {
	margin-top: 20rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.btn-feedback {
	margin: 0;
	padding: 0;
	background: none;
	font-size: 14pt;
	text-align: center;
	border: none;
	color: #888;
}
.btn-feedback:after {
	border: none;
}
.icon-fankuijianyi {
	font-size: 15pt;
	color: #888;
}

